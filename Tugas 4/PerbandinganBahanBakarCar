package main

import (
	"fmt"
)

type Car struct {
	Type   string
	FuelIn float64
}

func (c Car) PerkiraanJarakTempuh() float64 {
	PenggunaanBensin := 1.5 // liter per mill

	JarakTempuh := c.FuelIn * PenggunaanBensin
	return JarakTempuh
}

func main() {
	myCar := Car{
		Type:   "Sedan",
		FuelIn: 30, // in liters
	}

	JarakTempuh := myCar.PerkiraanJarakTempuh()
	fmt.Printf("Estimasi jarak yang dapat ditempuh: %.2f mill\n", JarakTempuh)
}
